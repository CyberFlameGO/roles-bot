{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist"]
		},
		"dev": {
			"outputs": ["dist"]
		},
		"preview": {
			"outputs": ["dist"]
		},
		"deploy": {
			"dependsOn": ["build", "test", "lint"]
		},
		"test": {
			"dependsOn": ["^build"]
		}
	}
}
